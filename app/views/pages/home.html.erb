<div class="jumbotron mapbox" id="map">
</div>

<div class="container-fluid">

	<div class="col-md-4" id="business">
	  <% @businesses.each do |i| %>
	  <div class="panel panel-default" id="businesses">
	    <div class="panel-heading">
	      <h3 class="panel-title" id="tit"><%= link_to i.name, business_path(i) %></h3>
	    </div>
	    <div class="panel-body">
	    	<%= i.description %>
	    </div>
	  </div><br>
	  <% end %>

	</div>


</div>
